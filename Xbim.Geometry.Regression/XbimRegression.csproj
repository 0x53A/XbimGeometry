  </ItemGroup>
  <ItemGroup>
    <None Include="..\Xbim.Geometry.Engine.Interop\bin\$(Configuration)\netcoreapp3.1\Xbim.Geometry.Engine32.dll" Link="Xbim.Geometry.Engine32.dll">
